{
  "name": "core-node",
  "type": "module",
  "version": "1.0.0",
  "description": "ZAR Core services server application written in node.",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "bun --hot config.js",
    "console": "bun --inspect=4000 config.js",
    "test": "bun test --preload ./test/setup.js",
    "migrate:up": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migration:generate": "sequelize-cli migration:generate --name "
  },
  "author": "GOLDEN AGE DEV LAB LTD. PTE.",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "glob": "^11.0.0",
    "hono": "^4.6.3",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "urlpattern-polyfill": "^10.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.2",
    "@types/bun": "latest",
    "babel-register": "^6.26.0",
    "bun-types": "latest",
    "sequelize-cli": "^6.6.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
